@model List<Person>
@{
    Layout = null;
}
 
@if(Model != null)
{
    <table style="width:100%;table-layout:auto">
    @foreach (var n in Model)
    { 
        <tr>
            <td style="width:5%">
                    <div>@n.Number</div>
            </td>
                <td style="width:25%">
                    <div>@n.RandomId</div>
                </td>
                <td style="width:20%">
                    <div>@n.FullName</div>
                </td>
                <td style="width:40%">
                    <div>@n.Adress</div>
                </td>
                <td style="width:10%">
                    <div>@n.Phone</div>
                </td> 

        </tr> 
            
    }
    </table>
} 